{
  "env": {
    "browser": true,
    "es2020": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "settings": {
    "import/resolver": "webpack"
  },
  "rules": {
    "new-cap": 0,
    "no-unused-vars": 0,
    "no-param-reassign": 0,

    "no-console": 0,
    "operator-linebreak": ["error", "after"]
  }
}
